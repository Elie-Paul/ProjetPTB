{% extends 'base.html.twig' %}

{% block title %}StockPtb{% endblock %}

{% block body %}
<div class="box box-default">
          <div class="box-header with-border">
          <div class="col-md-6">
            <h4><i class="fa fa-list"></i>&nbsp;<big>Stock Billet PTB</big></h4>
          </div>
          <div class="col-md-6 text-right">
            <a class="btn btn-info" href="{{ path('stock_ptb_lister') }}"><i class="fa fa-list"></i>&nbsp; Liste Stock Ptb</a>
          </div>
          </div>
          <!-- /.box-body -->
        </div>
<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Information du Stock</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ stock_ptb.id }}</td>
            </tr>
            <tr>
                <th>Nbre</th>
                <td>{{ stock_ptb.nbre }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ stock_ptb.createdAt ? stock_ptb.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ stock_ptb.updatedAt ? stock_ptb.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-warning" href="{{ path('stock_ptb_modifierst', {'id': stock_ptb.id}) }}"><i class="glyphicon glyphicon-pencil"></i>&nbsp;Modifier</a>
</div>
</div>

    {{ include('stock_ptb/_delete_form.html.twig') }}
{% endblock %}
