{% extends 'base.html.twig' %}

{% block title %}New BilletNavette{% endblock %}

{% block body %}
<h1><big>Gestion des Billets Autorail</big></h1>
<div class="box box-default">
  <div class="box-header with-border">
    <!--a class="btn btn-info" href="{{ path('billet_navette_index') }}">Retour</a-->
    <div class="col-md-6">
      <h4><i class="fa fa-plus"></i>&nbsp;<big>Créer un billet Autorail</big></h4>
    </div>
  </div>
</div>
<div class="box box-solid box-primary">
    <div class="box-header">
        <h3 class="box-title">Ajouter un Billet Autorail </h3>   
    </div>
    <div class="box-body">
      <div class="container">
        {{ include('billet_navette/_form.html.twig') }}
      </div>
    </div>
</div>

{% endblock %}
{% block javascripts %}
  <script>
    {% if error is defined %}
      swal("Attention", "Ce billet Autorail existe déjà", "warning");
    {% endif %}
  </script>
{% endblock %}
