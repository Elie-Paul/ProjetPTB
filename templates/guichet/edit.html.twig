{% extends 'base.html.twig' %}
{% block stylesheets %}
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css"/>
{% endblock %}
{% block title %}Edit Guichet{% endblock %}

{% block body %}

   <div class="box box-default">
          <div class="box-header with-border">
          <div class="col-md-6">
            <h4></h4>
          </div>
            <div class="col-md-6 text-right">
            <a class="btn btn-info pull-right" href="{{ path('guichet_index') }}"><i class="fa fa-list"></i>  <big>Liste des Guichets</big></a>
            </div>
          </div>
          <!-- /.box-body -->
        </div>
<div class="box box-solid box-primary">
    <div class="box-header">
        <h3 class="box-title">Modifier le guichet {{guichet.nom}}</h3>
    </div>
    <div class="box-body">
        {{ form_start(form) }}
        {{ form_widget(form) }}
            <button class="btn btn-warning"><big>{{ button_label|default('Modifier') }}</big></button>
        {{ form_end(form) }}
    </div>
</div>


    {{ include('guichet/_delete_form.html.twig') }}
{% endblock %}
