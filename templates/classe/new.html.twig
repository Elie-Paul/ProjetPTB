{% extends 'base.html.twig' %}

{% block title %}New Classe{% endblock %}

{% block body %}
<div class="box box-default">
          <div class="box-header with-border">
          <div class="col-md-6">
            <h4><i class="glyphicon glyphicon-plus"></i>&nbsp;<big>Ajouter Nouvelle Classe</big></h4>
          </div>
          <div class="col-md-6 text-right">
            <a class="btn btn-info" href="{{ path('classe_index') }}"><i class="fa fa-list"></i>&nbsp;Liste Classe</a>
          </div>
          </div>
</div>
<div class="box box-solid box-primary">
    <div class="box-header">
        <h3 class="box-title">Ajouter une nouvelle classe</h3>   
    </div>
    <div class="box-body">
        {{ include('classe/_form.html.twig') }}
    </div>
</div>
    

{% endblock %}
{% block javascripts %}
  <script>
    {% if error is defined %}
    swal("Attention", "Cette classe existe déjà", "warning");
  {% endif %}
  </script
{% endblock %}
